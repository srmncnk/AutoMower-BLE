    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Find my Huski",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/huski/findmyhuski/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Backend",
            "type": "dart",
            "request": "launch",
            "program": "${workspaceFolder}/huski/backend/lib/main.dart",
            "cwd": "${workspaceFolder}/huski/backend/",
            "args": [
                "--config=${workspaceFolder}/huski/backend/config/config.yml",
            ]
        },
    ]
}